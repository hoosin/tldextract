{
  "compilerOptions": {
    "module": "esnext",
    "target": "es6",

    "resolveJsonModule": true,

    "rootDir": "./",


    "sourceMap": true,
    "typeRoots": ["./types", "./node_modules/@types"],



    "moduleResolution": "node",
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true,


    "outDir": "./dist",




  },
  "include": ["lib/**/*.ts", "types/**/*.d.ts", "index.ts"],
  "exclude": ["node_modules", "**/*.test.ts", "dist"]
}
